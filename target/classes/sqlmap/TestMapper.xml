<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- JAVA와 연결할 Mapper 파일 설정 -->
<mapper namespace="TestDao">
	
	<!-- 연결 테스트 -->
	<select id="getTest" parameterType="TestDto" resultType="String">
		SELECT 	
			test
		FROM
			tn_test
	</select>
		
</mapper>